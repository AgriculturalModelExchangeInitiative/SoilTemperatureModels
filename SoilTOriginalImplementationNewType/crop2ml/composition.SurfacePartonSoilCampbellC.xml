        <Model name="SurfaceTemperatureParton" id="Crop2ML.SoilTOriginalImplementationNewType.SurfaceTemperatureParton" filename="unit.SurfaceTemperatureParton.xml"/>
        <Model name="SoilTemperatureCampbell" id="Crop2ML.SoilTOriginalImplementationNewType.SoilTemperatureCampbell" filename="unit.SoilTemperatureCampbell.xml"/>
        <Links>
            <InputLink target="SurfaceTemperatureParton.DayLength" source="DayLength"/>
            <InputLink target="SurfaceTemperatureParton.AirTemperatureMaximum" source="AirTemperatureMaximum"/>
            <InputLink target="SurfaceTemperatureParton.AirTemperatureMinimum" source="AirTemperatureMinimum"/>
            <InputLink target="SurfaceTemperatureParton.AboveGroundBiomass" source="AboveGroundBiomass"/>
            <InputLink target="SurfaceTemperatureParton.GlobalSolarRadiation" source="GlobalSolarRadiation"/>
            <InputLink target="SoilTemperatureCampbell.Clay" source="Clay"/>
            <InputLink target="SoilTemperatureCampbell.ThermalK" source="ThermalK"/>
            <InputLink target="SoilTemperatureCampbell.VolumetricWaterContent" source="VolumetricWaterContent"/>
            <InputLink target="SoilTemperatureCampbell.DepthConstantSoilTemperature" source="DepthConstantSoilTemperature"/>
            <InputLink target="SoilTemperatureCampbell.Timestep" source="Timestep"/>
            <InputLink target="SoilTemperatureCampbell.BottomTemperature" source="BottomTemperature"/>
            <InputLink target="SoilTemperatureCampbell.BulkDensity" source="BulkDensity"/>
            <InputLink target="SoilTemperatureCampbell.NetRadiation" source="NetRadiation"/>
            <InputLink target="SoilTemperatureCampbell.ETA" source="ETA"/>
            <InputLink target="SoilTemperatureCampbell.LayerThickness" source="LayerThickness"/>
            <InternalLink target="SoilTemperatureCampbell.SurfaceTemperatureMaximum" source="SurfaceTemperatureParton.SurfaceTemperatureMaximum"/>
            <InternalLink target="SoilTemperatureCampbell.SurfaceSoilTemperature" source="SurfaceTemperatureParton.SurfaceSoilTemperature"/>
            <OutputLink target="SurfaceTemperatureMinimum" source="SurfaceTemperatureParton.SurfaceTemperatureMinimum"/>
            <OutputLink target="SurfaceSoilTemperature" source="SurfaceTemperatureParton.SurfaceSoilTemperature"/>
            <OutputLink target="SurfaceTemperatureMaximum" source="SurfaceTemperatureParton.SurfaceTemperatureMaximum"/>
            <OutputLink target="HeatCapacity" source="SoilTemperatureCampbell.HeatCapacity"/>
            <OutputLink target="SoilTemperatureByLayersChange" source="SoilTemperatureCampbell.SoilTemperatureByLayersChange"/>
            <OutputLink target="SoilTemperatureByLayers" source="SoilTemperatureCampbell.SoilTemperatureByLayers"/>
            <OutputLink target="LatentHeatFlux" source="SoilTemperatureCampbell.LatentHeatFlux"/>
