<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//SIMPLACE/DTD GRP 1.0//EN" "http://simplace.net/dtd/GroupComponent.dtd">
<configuration class="net.simplace.sim.components.Stics_soil_temperature.Soiltemp">
    <simgroup>
        <simcomponent id="temp_amp" class="net.simplace.sim.components.Stics_soil_temperature.temp_amp">
            <input id="max_temp" source="Soiltemp.max_temp"/>
            <input id="min_temp" source="Soiltemp.min_temp"/>
            <output id="temp_amp" destination="Soiltemp.temp_amp"/></simcomponent>
        <simcomponent id="therm_amp" class="net.simplace.sim.components.Stics_soil_temperature.therm_amp">
            <input id="therm_diff" source="Soiltemp.therm_diff"/>
            <input id="temp_wave_freq" source="Soiltemp.temp_wave_freq"/>
            <output id="therm_amp" destination="Soiltemp.therm_amp"/></simcomponent>
        <simcomponent id="temp_profile" class="net.simplace.sim.components.Stics_soil_temperature.temp_profile">
            <input id="prev_temp_profile" source="Soiltemp.prev_temp_profile"/>
            <input id="prev_canopy_temp" source="Soiltemp.prev_canopy_temp"/>
            <input id="min_air_temp" source="Soiltemp.min_air_temp"/>
            <input id="temp_amp" source="temp_amp.temp_amp"/>
            <input id="therm_amp" source="therm_amp.therm_amp"/>
            <output id="temp_profile" destination="Soiltemp.temp_profile"/></simcomponent></simgroup></configuration>