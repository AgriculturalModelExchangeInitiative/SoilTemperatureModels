<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//SIMPLACE/DTD GRP 1.0//EN" "http://simplace.net/dtd/GroupComponent.dtd">
<configuration class="net.simplace.sim.components.Monica_SoilTemp.SoilTemperatureComp">
    <simgroup>
        <simcomponent id="NoSnowSoilSurfaceTemperature" class="net.simplace.sim.components.Monica_SoilTemp.NoSnowSoilSurfaceTemperature">
            <input id="tmin" source="SoilTemperatureComp.tmin"/>
            <input id="tmax" source="SoilTemperatureComp.tmax"/>
            <input id="globrad" source="SoilTemperatureComp.globrad"/>
            <input id="dampingFactor" source="SoilTemperatureComp.dampingFactor"/>
            <input id="soilCoverage" source="SoilTemperatureComp.soilCoverage"/></simcomponent>
        <simcomponent id="WithSnowSoilSurfaceTemperature" class="net.simplace.sim.components.Monica_SoilTemp.WithSnowSoilSurfaceTemperature">
            <input id="soilSurfaceTemperatureBelowSnow" source="SoilTemperatureComp.soilSurfaceTemperatureBelowSnow"/>
            <input id="hasSnowCover" source="SoilTemperatureComp.hasSnowCover"/>
            <input id="noSnowSoilSurfaceTemperature" source="NoSnowSoilSurfaceTemperature.soilSurfaceTemperature"/>
            <output id="soilSurfaceTemperature" destination="SoilTemperatureComp.soilSurfaceTemperature"/></simcomponent>
        <simcomponent id="SoilTemperature" class="net.simplace.sim.components.Monica_SoilTemp.SoilTemperature">
            <input id="timeStep" source="SoilTemperatureComp.timeStep"/>
            <input id="soilMoistureConst" source="SoilTemperatureComp.soilMoistureConst"/>
            <input id="baseTemp" source="SoilTemperatureComp.baseTemp"/>
            <input id="initialSurfaceTemp" source="SoilTemperatureComp.initialSurfaceTemp"/>
            <input id="densityAir" source="SoilTemperatureComp.densityAir"/>
            <input id="specificHeatCapacityAir" source="SoilTemperatureComp.specificHeatCapacityAir"/>
            <input id="densityHumus" source="SoilTemperatureComp.densityHumus"/>
            <input id="specificHeatCapacityHumus" source="SoilTemperatureComp.specificHeatCapacityHumus"/>
            <input id="densityWater" source="SoilTemperatureComp.densityWater"/>
            <input id="specificHeatCapacityWater" source="SoilTemperatureComp.specificHeatCapacityWater"/>
            <input id="quartzRawDensity" source="SoilTemperatureComp.quartzRawDensity"/>
            <input id="specificHeatCapacityQuartz" source="SoilTemperatureComp.specificHeatCapacityQuartz"/>
            <input id="nTau" source="SoilTemperatureComp.nTau"/>
            <input id="noOfTempLayers" source="SoilTemperatureComp.noOfTempLayers"/>
            <input id="noOfSoilLayers" source="SoilTemperatureComp.noOfSoilLayers"/>
            <input id="layerThickness" source="SoilTemperatureComp.layerThickness"/>
            <input id="soilBulkDensity" source="SoilTemperatureComp.soilBulkDensity"/>
            <input id="saturation" source="SoilTemperatureComp.saturation"/>
            <input id="soilOrganicMatter" source="SoilTemperatureComp.soilOrganicMatter"/>
            <input id="V" source="SoilTemperatureComp.V"/>
            <input id="B" source="SoilTemperatureComp.B"/>
            <input id="volumeMatrix" source="SoilTemperatureComp.volumeMatrix"/>
            <input id="volumeMatrixOld" source="SoilTemperatureComp.volumeMatrixOld"/>
            <input id="matrixPrimaryDiagonal" source="SoilTemperatureComp.matrixPrimaryDiagonal"/>
            <input id="matrixSecondaryDiagonal" source="SoilTemperatureComp.matrixSecondaryDiagonal"/>
            <input id="heatConductivity" source="SoilTemperatureComp.heatConductivity"/>
            <input id="heatConductivityMean" source="SoilTemperatureComp.heatConductivityMean"/>
            <input id="heatCapacity" source="SoilTemperatureComp.heatCapacity"/>
            <input id="solution" source="SoilTemperatureComp.solution"/>
            <input id="matrixDiagonal" source="SoilTemperatureComp.matrixDiagonal"/>
            <input id="matrixLowerTriangle" source="SoilTemperatureComp.matrixLowerTriangle"/>
            <input id="heatFlow" source="SoilTemperatureComp.heatFlow"/>
            <input id="soilSurfaceTemperature" source="WithSnowSoilSurfaceTemperature.soilSurfaceTemperature"/>
            <output id="soilTemperature" destination="SoilTemperatureComp.soilTemperature"/></simcomponent></simgroup></configuration>