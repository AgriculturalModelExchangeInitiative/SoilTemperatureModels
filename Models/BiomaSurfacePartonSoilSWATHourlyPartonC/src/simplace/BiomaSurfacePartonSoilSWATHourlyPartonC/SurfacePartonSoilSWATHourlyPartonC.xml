<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//SIMPLACE/DTD GRP 1.0//EN" "http://simplace.net/dtd/GroupComponent.dtd">
<configuration class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.SurfacePartonSoilSWATHourlyPartonC">
    <simgroup>
        <simcomponent id="SurfaceTemperatureParton" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.SurfaceTemperatureParton">
            <input id="GlobalSolarRadiation" source="SurfacePartonSoilSWATHourlyPartonC.GlobalSolarRadiation"/>
            <input id="DayLength" source="SurfacePartonSoilSWATHourlyPartonC.DayLength"/>
            <input id="AboveGroundBiomass" source="SurfacePartonSoilSWATHourlyPartonC.AboveGroundBiomass"/>
            <input id="AirTemperatureMinimum" source="SurfacePartonSoilSWATHourlyPartonC.AirTemperatureMinimum"/>
            <input id="AirTemperatureMaximum" source="SurfacePartonSoilSWATHourlyPartonC.AirTemperatureMaximum"/>
            <output id="SurfaceSoilTemperature" destination="SurfacePartonSoilSWATHourlyPartonC.SurfaceSoilTemperature"/>
            <output id="SurfaceTemperatureMinimum" destination="SurfacePartonSoilSWATHourlyPartonC.SurfaceTemperatureMinimum"/>
            <output id="SurfaceTemperatureMaximum" destination="SurfacePartonSoilSWATHourlyPartonC.SurfaceTemperatureMaximum"/></simcomponent>
        <simcomponent id="VolumetricHeatCapacityKluitenberg" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.VolumetricHeatCapacityKluitenberg">
            <input id="VolumetricWaterContent" source="SurfacePartonSoilSWATHourlyPartonC.VolumetricWaterContent"/>
            <input id="Sand" source="SurfacePartonSoilSWATHourlyPartonC.Sand"/>
            <input id="BulkDensity" source="SurfacePartonSoilSWATHourlyPartonC.BulkDensity"/>
            <input id="OrganicMatter" source="SurfacePartonSoilSWATHourlyPartonC.OrganicMatter"/>
            <input id="Clay" source="SurfacePartonSoilSWATHourlyPartonC.Clay"/>
            <input id="Silt" source="SurfacePartonSoilSWATHourlyPartonC.Silt"/>
            <output id="HeatCapacity" destination="SurfacePartonSoilSWATHourlyPartonC.HeatCapacity"/></simcomponent>
        <simcomponent id="ThermalConductivitySIMULAT" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.ThermalConductivitySIMULAT">
            <input id="VolumetricWaterContent" source="SurfacePartonSoilSWATHourlyPartonC.VolumetricWaterContent"/>
            <input id="Clay" source="SurfacePartonSoilSWATHourlyPartonC.Clay"/>
            <input id="BulkDensity" source="SurfacePartonSoilSWATHourlyPartonC.BulkDensity"/>
            <output id="ThermalConductivity" destination="SurfacePartonSoilSWATHourlyPartonC.ThermalConductivity"/></simcomponent>
        <simcomponent id="SoilTemperatureSWAT" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.SoilTemperatureSWAT">
            <input id="VolumetricWaterContent" source="SurfacePartonSoilSWATHourlyPartonC.VolumetricWaterContent"/>
            <input id="BulkDensity" source="SurfacePartonSoilSWATHourlyPartonC.BulkDensity"/>
            <input id="LayerThickness" source="SurfacePartonSoilSWATHourlyPartonC.LayerThickness"/>
            <input id="LagCoefficient" source="SurfacePartonSoilSWATHourlyPartonC.LagCoefficient"/>
            <input id="AirTemperatureAnnualAverage" source="SurfacePartonSoilSWATHourlyPartonC.AirTemperatureAnnualAverage"/>
            <input id="SoilProfileDepth" source="SurfacePartonSoilSWATHourlyPartonC.SoilProfileDepth"/>
            <input id="SurfaceSoilTemperature" source="SurfaceTemperatureParton.SurfaceSoilTemperature"/>
            <output id="SoilTemperatureByLayers" destination="SurfacePartonSoilSWATHourlyPartonC.SoilTemperatureByLayers"/></simcomponent>
        <simcomponent id="ThermalDiffu" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.ThermalDiffu">
            <input id="HeatCapacity" source="VolumetricHeatCapacityKluitenberg.HeatCapacity"/>
            <input id="ThermalConductivity" source="ThermalConductivitySIMULAT.ThermalConductivity"/>
            <output id="ThermalDiffusivity" destination="SurfacePartonSoilSWATHourlyPartonC.ThermalDiffusivity"/></simcomponent>
        <simcomponent id="RangeOfSoilTemperaturesDAYCENT" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.RangeOfSoilTemperaturesDAYCENT">
            <input id="LayerThickness" source="SurfacePartonSoilSWATHourlyPartonC.LayerThickness"/>
            <input id="SurfaceTemperatureMaximum" source="SurfaceTemperatureParton.SurfaceTemperatureMaximum"/>
            <input id="SurfaceTemperatureMinimum" source="SurfaceTemperatureParton.SurfaceTemperatureMinimum"/>
            <input id="SoilTemperatureByLayers" source="SoilTemperatureSWAT.SoilTemperatureByLayers"/>
            <input id="ThermalDiffusivity" source="ThermalDiffu.ThermalDiffusivity"/>
            <output id="SoilTemperatureRangeByLayers" destination="SurfacePartonSoilSWATHourlyPartonC.SoilTemperatureRangeByLayers"/>
            <output id="SoilTemperatureMinimum" destination="SurfacePartonSoilSWATHourlyPartonC.SoilTemperatureMinimum"/>
            <output id="SoilTemperatureMaximum" destination="SurfacePartonSoilSWATHourlyPartonC.SoilTemperatureMaximum"/></simcomponent>
        <simcomponent id="HourlySoilTemperaturesPartonLogan" class="net.simplace.sim.components.BiomaSurfacePartonSoilSWATHourlyPartonC.HourlySoilTemperaturesPartonLogan">
            <input id="HourOfSunrise" source="SurfacePartonSoilSWATHourlyPartonC.HourOfSunrise"/>
            <input id="HourOfSunset" source="SurfacePartonSoilSWATHourlyPartonC.HourOfSunset"/>
            <input id="DayLength" source="SurfacePartonSoilSWATHourlyPartonC.DayLength"/>
            <input id="SoilTemperatureMinimum" source="RangeOfSoilTemperaturesDAYCENT.SoilTemperatureMinimum"/>
            <input id="SoilTemperatureMaximum" source="RangeOfSoilTemperaturesDAYCENT.SoilTemperatureMaximum"/>
            <output id="SoilTemperatureByLayersHourly" destination="SurfacePartonSoilSWATHourlyPartonC.SoilTemperatureByLayersHourly"/></simcomponent></simgroup></configuration>